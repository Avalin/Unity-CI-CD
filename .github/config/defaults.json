{
  "unity": {
    "version": "auto"
  },
  "project": {
    "name": "My_Project"
  },
  "pipeline": {
    "useGitLfs": false,
    "quietMode": false,
    "excludeUnityTests": false,
    "forceCombineArtifacts": false,
    "freeDiskSpace": "auto"
  },
  "tests": {
    "editMode": {
      "path": "Assets/Tests/Editor"
    },
    "playMode": {
      "path": "Assets/Tests/PlayMode"
    },
    "timeoutMinutes": 15
  },
  "build": {
    "timeoutMinutes": 30,
    "retentionDays": {
      "preview": 7,
      "release_candidate": 14,
      "release": 30
    },
    "defaultTargets": [
      "WebGL"
    ],
    "availableTargets": [
      "Android",
      "WebGL",
      "StandaloneLinux64-Client",
      "StandaloneLinux64-Server",
      "StandaloneWindows",
      "StandaloneWindows64",
      "StandaloneOSX",
      "iOS"
    ],
    "freeDiskSpaceAutoTargets": ["Android"]
  },
  "deploy": {
    "defaultTargets": [
      "gh-pages"
    ],
    "availableTargets": [
      "itch.io",
      "appcenter",
      "firebase",
      "s3",
      "gh-pages",
      "steam",
      "testflight",
      "custom-server"
    ]
  }
}